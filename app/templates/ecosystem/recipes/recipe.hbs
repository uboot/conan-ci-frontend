<h2>{{model.name}}/{{model.version}}@{{model.user}}/{{model.channel}}</h2>

<p>
  {{model.revision}}
</p>

<section>
  <div class="row row-cols-1">
    <table class="table">
      <thead>
        <tr>
          <th scope="col">Package ID</th>
        </tr>
      </thead>
      <tbody>
        {{#each this.model.packages as |package|}}
          <tr>
            <td>{{package.package_id}}</td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</section>

